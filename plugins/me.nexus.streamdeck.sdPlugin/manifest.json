{
  "$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
  "Name": "NEXUS Control",
  "Version": "1.0.0",
  "Author": "NEXUS Team",
  "Description": "Control NEXUS multi-agent AI system from your Stream Deck. Spawn agents, execute quick actions, and monitor system status.",
  "Icon": "images/nexus-plugin",
  "URL": "https://github.com/nexus-ai/nexus",
  "UUID": "me.nexus.streamdeck",
  "SDKVersion": 2,
  "CodePath": "plugin.js",
  "Software": {
    "MinimumVersion": "6.0"
  },
  "OS": [
    {
      "Platform": "linux",
      "MinimumVersion": "1.0"
    },
    {
      "Platform": "mac",
      "MinimumVersion": "10.15"
    },
    {
      "Platform": "windows",
      "MinimumVersion": "10"
    }
  ],
  "Nodejs": {
    "Version": "18",
    "Debug": "enabled"
  },
  "Category": "NEXUS AI",
  "CategoryIcon": "images/nexus-category",
  "Actions": [
    {
      "Name": "Spawn Orchestrator",
      "UUID": "me.nexus.streamdeck.spawn-orchestrator",
      "Icon": "icons/orchestrator",
      "Tooltip": "Spawn an Orchestrator agent to coordinate tasks",
      "PropertyInspectorPath": "property-inspector/spawn-agent.html",
      "States": [
        {
          "Image": "icons/orchestrator",
          "TitleAlignment": "bottom",
          "FontSize": "10"
        }
      ],
      "Controllers": ["Keypad"]
    },
    {
      "Name": "Spawn Architect",
      "UUID": "me.nexus.streamdeck.spawn-architect",
